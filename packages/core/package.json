{
	"name": "@terminallyonline/cord",
	"version": "0.0.11",
	"description": "A templating language for creating structured, type-safe input forms",
	"license": "MIT",
	"homepage": "https://github.com/Terminally-Online/cord#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Terminally-Online/cord.git"
	},
	"bugs": {
		"url": "https://github.com/Terminally-Online/cord/issues"
	},
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "rm -rf dist && tsc && tsc --project tsconfig.esm.json",
		"dev": "tsc --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"peerDependencies": {
		"react": "^18.0.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.45",
		"react": "^18.2.0",
		"typescript": "^5.3.3",
		"vitest": "^2.1.5"
	},
	"files": [
		"dist",
		"README.md"
	],
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/cjs/index.js"
		},
		"./react": {
			"types": "./dist/types/react/index.d.ts",
			"import": "./dist/esm/react/index.js",
			"require": "./dist/cjs/react/index.js",
			"default": "./dist/cjs/react/index.js"
		}
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/types/index.d.ts",
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"templating",
		"forms",
		"validation",
		"ethereum",
		"blockchain",
		"typescript",
		"react"
	]
}
