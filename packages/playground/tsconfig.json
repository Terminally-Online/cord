{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "jsx": "react-jsx",
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@cord/core": [
                "../core/src"
            ],
            "@cord/core/react": [
                "../core/src/react"
            ]
        }
    },
    "include": [
        "src"
    ],
    "references": [
        {
            "path": "../core"
        }
    ]
}
